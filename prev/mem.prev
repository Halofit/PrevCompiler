(
	a[1] = -21,
	p = @a[0],
	p^ = 1,
	for i=0,9 : (		
		printInt(i),
		printStr(": "),
		printInt(a[i]),
		printStr("\t")
	) end,
	printStr("\n"),
	123
) where
	var i : integer
	var p : ptr integer
